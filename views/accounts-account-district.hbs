<h1 class="mb-4">{{title}}</h1>

<form method="GET" action="/api/account-district" id="form-fnd-account-district">
  <div class="input-group mb-4">
    <input type="text" name="account_id" placeholder="Enter an Account ID..." class="form-control" id="account_id">
    <div class="input-group-append">
      <button type="submit" class="btn btn-outline-primary">Search</button>
    </div>
  </div>
  <div class="form-check">
    <input type="checkbox" name="optimized" id="optimized" class="form-check-input">
    <label for="optimized" class="form-check-label">Use optimized query</label>
  </div>
</form>

<br>

<p class="d-none" id="query-time-text">Query took <span id="query-time-val"></span> ms</p>
<div id="account-district-table"></div>
<h3 class="text-center d-none" id="no-records">No records found</h3>

<script src="/js/commons.bundle.js"></script>
<script src="/js/accounts-account-district.bundle.js"></script>
<h1 class="mb-4">{{title}}</h1>

<form method="GET" action="/api/account-district" id="form-fnd-account-district">
  <div class="input-group mb-4">
    <input type="search" name="account_id" placeholder="Enter an Account ID..."
      class="form-control" id="account_id">
    <div class="input-group-append">
      <button type="submit" class="btn bg-yellow">Search</button>
    </div>
  </div>
</form>

<div id="account-district-table"></div>
<h3 class="text-center d-none" id="no-records">No records found</h3>

<script src="/js/commons.bundle.js"></script>
<script src="/js/accounts-account-district.bundle.js"></script>
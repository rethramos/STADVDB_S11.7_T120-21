<h1 class="mb-4">{{title}}</h1>

<form method="GET" action="/api/salary" id="form-avg-salary">
  <div class="input-group mb-4">
    <input type="search" name="region" placeholder="Enter a region, or a comma-separated string of regions..."
      class="form-control" id="region">
    <div class="input-group-append">
      <button type="submit" class="btn bg-yellow">Search</button>
    </div>
  </div>
</form>

<div id="salary-table" class="d-none"></div>
<h3 class="text-center d-none" id="no-records">No records found</h3>

<script src="/js/commons.bundle.js"></script>
<script src="/js/districts-salary.bundle.js"></script>